{
  "name": "github-to-md",
  "version": "0.7.5",
  "description": "Export Github (Issues, Stars, Following) to markdown file",
  "main": "app.js",
  "scripts": {
    "prettier": "prettier --write \"./**/*.{ts,js,md,json}\"",
    "watch": "tsc -w",
    "dev": "ts-node ./src/app.ts",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "bin": "bin/app.js",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainbow-design/github-export.git"
  },
  "keywords": [
    "github",
    "issues",
    "markdown",
    "export",
    "blog",
    "issue",
    "star",
    "follow"
  ],
  "author": "yanyue404",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rainbow-design/github-export/issues"
  },
  "homepage": "https://github.com/rainbow-design/github-export#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "commander": "^6.0.0",
    "downloadjs": "^1.4.7",
    "filenamify": "^4.1.0",
    "markdown-js": "^0.0.4",
    "markdown-table": "^1.1.3",
    "turndown": "^5.0.3",
    "turndown-plugin-gfm": "^1.0.2"
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
}
