{
  "name": "issues-to-markdown",
  "version": "0.0.5",
  "description": "Convert github issues to md files",
  "main": "app.js",
  "scripts": {
    "prettier": "prettier --write \"./**/*.{ts,js,md,json}\"",
    "watch": "tsc -w",
    "log:issues": "ts-node ./helpers/issues.ts",
    "export:articles": "ts-node ./helpers/articles.ts",
    "export:toc": "ts-node ./helpers/toc.ts",
    "export:following": "ts-node ./helpers/following.ts",
    "export:stars": "ts-node ./helpers/stars.ts",
    "server": "nodemon ./build/app.js",
    "start": "npm run server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanyue404/issues-to-markdown.git"
  },
  "keywords": [
    "github",
    "issues",
    "markdown",
    "export"
  ],
  "author": "yanyue404",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yanyue404/issues-to-markdown/issues"
  },
  "homepage": "https://github.com/yanyue404/issues-to-markdown#readme",
  "dependencies": {
    "@types/express": "^4.17.2",
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "downloadjs": "^1.4.7",
    "express": "^4.17.1",
    "filenamify": "^4.1.0",
    "markdown-js": "^0.0.4",
    "markdown-table": "^1.1.3",
    "turndown": "^5.0.3",
    "turndown-plugin-gfm": "^1.0.2"
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
}
